<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors>Cenngo</Authors>
    <Description>.NET Wrapper for R6Tab Api</Description>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/Cenngo/R6Tab.NET</PackageProjectUrl>
    <RepositoryType>Public</RepositoryType>
    <PackageReleaseNotes>General Overhaul to the Operator Data Retrieval and Changes to Accommodate new Api Key Requirements</PackageReleaseNotes>
    <Version>0.4.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyName>R6Tab.NET</AssemblyName>
    <RootNamespace>R6Api</RootNamespace>
    <RepositoryUrl>https://github.com/Cenngo/R6Tab.NET</RepositoryUrl>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>C:\Users\Cenk\source\repos\R6Tab.NET\R6TabApiWrapper\R6Tab.NET.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="2.9.6">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

</Project>
